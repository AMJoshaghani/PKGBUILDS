sha512sums=('1b82219f67bac38e8177ef17e3ae316313d59c92470c92fc0272953c977aa3259fd61d7b8dffd8d24c1c5e3004e8c97d84d9825832e14d087f7e0c70eaa9937d')

package() {
 install -d $pkgdir/usr/bin
 install -Dm755 eos-rankmirrors $pkgdir/usr/bin/eos-rankmirrors
}
